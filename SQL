# vypise nazvy her, ktere podporuji cestinu
SELECT
  gameName, "supported languages"
  FROM test 
  WHERE REGEXP_LIKE("supported languages",'Czech')

# vypise vsechny multiplayer hry
SELECT
  gameName, __time
FROM test
WHERE __time >= (TIME_PARSE('2020-01-01T00:00:00Z')) AND __time <= (TIME_PARSE('2020-12-31T23:59:00Z')) 
# vypise vsechny hry podporujici linux
SELECT
  gameName, tags
  FROM test 
  WHERE linux = 1 

 
 
 

